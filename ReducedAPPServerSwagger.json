            "description": "Ident_OrgRoot = 'Application Role'",
            "required": false,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Collection entry",
            "schema": {
                "required": [
                  "uri"
                ],
                "properties": {
                  "uri": {
                    "type": "string",
                    "example": "/AppServer/api/entity/OrgRoot/QER-V-AERole"
                  },
                  "display": {
